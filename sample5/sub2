int add(int a, int b){

        int ans;

        ans = a + b;
        return ans;
}

int sub(int a, int b){

        int ans;

        ans = a -  b;
        return ans;
}

int mul(int a, int b){

        int ans;

        ans = a * b;
        return ans;
}

int div(int a, int b){

        int ans;

        ans = a / b;
        return ans;
}

int mod(int a, int b){

        int ans;

        ans = a % b;
        return ans;
}
